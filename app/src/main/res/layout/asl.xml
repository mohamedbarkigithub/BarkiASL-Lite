<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="@drawable/gradient_asl"
	android:id="@+id/gradientlayout"
	android:orientation="vertical"
	android:fitsSystemWindows="true"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="5dp"
		tools:ignore="UselessLeaf" />
	<LinearLayout
		android:id="@+id/maindLinearLayoutEditText"
		android:gravity="center"
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		<ImageButton
			android:id="@+id/maindButton1"
			android:layout_width="50.0dip"
			android:layout_height="50.0dip"
			android:background="@drawable/button_c"
			android:src="@drawable/ic_video_vintage"
			android:contentDescription="@string/video_or_reload"
			app:tint="@color/button_c"
			tools:ignore="ImageContrastCheck,SpeakableTextPresentCheck" />
		<RelativeLayout
			android:layout_weight="1.0"
			android:background="@drawable/button_statu"
			android:paddingLeft="5.0dip"
			android:layout_width="0dp"
			android:layout_marginLeft="2dp"
			android:layout_marginRight="2dp"
			android:layout_height="52.0dp"
			tools:ignore="RtlHardcoded,RtlSymmetry">
			<com.mohamed.barki.asl.lite.AutoCompleteEditText
				android:id="@+id/maindEditText1"
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:layout_centerVertical="true"
				android:layout_marginLeft="5dp"
				android:layout_marginRight="5dp"
				android:background="@android:color/transparent"
				android:cursorVisible="true"
				android:digits="@string/ar_lang"
				android:drawableRight="@drawable/ic_menu_search"
				android:drawablePadding="10dp"
				android:drawableTint="@color/textPrimaryInverse"
				android:drawableTintMode="multiply"
				android:dropDownHeight="wrap_content"
				android:hint="@string/search"
				android:imeOptions="actionDone"
				android:inputType="text|textAutoComplete|textNoSuggestions"
				android:scrollHorizontally="true"
				android:singleLine="true"
				android:textColor="@color/colorText"
				android:textColorHint="@color/colorTextHint"
				android:textCursorDrawable="@drawable/color_cursor"
				android:textIsSelectable="false"
				android:typeface="monospace"
				android:textAppearance="?android:attr/textAppearanceLarge"
				android:popupBackground="@android:color/transparent"
				tools:ignore="TextContrastCheck,TouchTargetSizeCheck,VisualLintTextFieldSize,DuplicateSpeakableTextCheck" />
			<View
				android:id="@+id/maindButtonClose"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:background="@drawable/button_close"
				android:visibility="visible"
				android:contentDescription="@string/search_btn"
				tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
		</RelativeLayout>

		<RelativeLayout
			android:id="@+id/maindrlvSpinner1"
			android:layout_width="50.0dp"
			android:layout_height="50.0dp"
			android:layout_centerVertical="true"
			tools:ignore="ObsoleteLayoutParam">

			<Spinner
				android:id="@+id/maindSpinner1"
				android:layout_width="50.0dp"
				android:layout_height="50.0dp"
				android:layout_span="2"
				android:background="@android:color/transparent"
				android:dropDownSelector="#ffffffff"
				android:gravity="center"
				android:padding="3dp"
				android:popupBackground="@android:color/transparent"
				android:spinnerMode="dropdown"
				android:textSize="19.0sp"
				android:clickable="false"
				android:visibility="invisible"
				android:contentDescription="@string/spinner"
				tools:ignore="ObsoleteLayoutParam,TextSizeCheck,DuplicateClickableBoundsCheck" />

			<ImageButton
				android:id="@+id/imgSpinner1"
				android:layout_width="50dp"
				android:layout_height="50dp"
				android:background="@drawable/background_cell"
				android:src="@drawable/ic_spinner"
				android:contentDescription="@string/spinner_img"
				app:tint="@color/cell"
				tools:ignore="SpeakableTextPresentCheck,ImageContrastCheck" />
		</RelativeLayout>
	</LinearLayout>
	<LinearLayout
		android:orientation="vertical"
		android:gravity="center"
		android:layout_marginTop="5dp"
		android:layout_marginLeft="5dp"
		android:layout_marginRight="5dp"
		android:background="@drawable/background_top"
		android:id="@+id/maindLinearLayout33"
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		<LinearLayout
			android:gravity="left"
			android:orientation="vertical"
			android:id="@+id/maindTextViewL11"
			android:background="@android:color/transparent"
			android:paddingLeft="5.0dip"
			android:paddingRight="5.0dip"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			tools:ignore="RtlHardcoded">
			<TextView
				android:textAppearance="?android:textAppearanceMedium"
				android:id="@+id/maindTextView11"
				android:gravity="right"
				android:background="@android:color/transparent"
				android:textColor="#ff000000"
				android:paddingLeft="2.0dip"
				android:paddingRight="2.0dip"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:textIsSelectable="false"/>
		</LinearLayout>
		<LinearLayout
			android:gravity="right"
			android:orientation="vertical"
			android:id="@+id/maindTextViewL22"
			android:background="@android:color/transparent"
			android:paddingLeft="5.0dp"
			android:paddingBottom="2.0dip"
			android:paddingRight="5.0dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			tools:ignore="RtlHardcoded">
			<TextView
				android:textAppearance="?android:textAppearanceMedium"
				android:id="@+id/maindTextView22"
				android:gravity="left"
				android:background="@android:color/transparent"
				android:textColor="#ff000000"
				android:padding="2.0dp"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:textIsSelectable="false"/>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:gravity="center|top"
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_marginTop="5dp"
		android:layout_marginLeft="5dp"
		android:layout_marginRight="5dp"
		android:layout_weight="1.0">
		<LinearLayout
			android:orientation="vertical"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			tools:ignore="UselessParent">
			<LinearLayout
				android:background="@drawable/button_scor"
				android:id="@+id/mainLinearLayoutScor"
				android:gravity="center"
				android:orientation="horizontal"
				android:padding="15dp"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginBottom="5dp"
				android:layout_marginTop="10.0dip"
				tools:ignore="VisualLintBounds">
				<TextView
					android:background="@drawable/button_scor"
					android:id="@+id/mainTextViewScorL"
					android:visibility="gone"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:padding="5dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="10.0dip"
					android:textColor="@color/scor"
					android:typeface="monospace"
					android:textStyle="bold"/>
				<TextView
					android:id="@+id/mainTextViewScor"
					android:text="@string/scor"
					android:textColor="@color/scor"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:layout_width="0dp"
					android:layout_height="wrap_content"
					android:layout_weight="1.0"
					android:layout_marginLeft="10.0dip"
					android:textStyle="bold"
					tools:ignore="RtlHardcoded" />
				<TextView
					android:background="@drawable/button_scor"
					android:id="@+id/mainTextViewScorR"
					android:visibility="gone"
					android:textAppearance="?android:attr/textAppearanceMedium"
					android:padding="5dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginRight="10.0dip"
					android:textColor="@color/scor"
					android:typeface="monospace"
					android:textStyle="bold"/>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/maindLinearLayout4"
				android:gravity="center"
				android:layout_width="match_parent"
				android:layout_height="0dp"
				android:layout_weight="1.0"
				android:orientation="vertical"
				android:weightSum="1">
				<LinearLayout
					android:id="@+id/maindLinearLayout0"
					android:layout_marginBottom="10dp"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="0.25"
					android:gravity="center"
					android:layout_marginTop="5dp"
					tools:ignore="NestedWeights">
					<ImageButton
						android:id="@+id/maindButtonImageInvisible"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:visibility="invisible"
						android:contentDescription="@string/smile_gone_invisible"
						tools:ignore="SpeakableTextPresentCheck" />
					<androidx.constraintlayout.widget.ConstraintLayout
						android:layout_width="wrap_content"
						android:layout_height="match_parent">
						<ImageView
							android:id="@+id/maindButtonView"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							app:layout_constraintDimensionRatio="1:1"
							app:layout_constraintEnd_toEndOf="parent"
							app:layout_constraintStart_toStartOf="parent"
							tools:ignore="MissingConstraints,ImageContrastCheck" />
					</androidx.constraintlayout.widget.ConstraintLayout>
					<ImageButton
						android:id="@+id/maindButtonImage"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:background="@android:color/transparent"
						android:src="@drawable/ic_smalle"
						android:contentDescription="@string/smile_gone"
						app:tint="@color/textPrimaryInverse"
						tools:ignore="SpeakableTextPresentCheck" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/maindLinearLayout1Big"
					android:layout_width="match_parent"
					android:layout_height="0dp"
					android:layout_weight="0.75"
					android:orientation="vertical"
					android:gravity="bottom">
					<LinearLayout
						android:id="@+id/maindLinearLayout1"
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1.0"
						tools:ignore="Orientation">
						<ImageView
							android:id="@+id/maindImageView"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:visibility="gone"
							android:clickable="false"
							android:saveEnabled="false"
							android:focusable="false"
							android:focusableInTouchMode="false"
							android:focusedByDefault="false"
							android:touchscreenBlocksFocus="false"
							android:screenReaderFocusable="false"
							android:layout_gravity="center"
							android:scaleType="fitXY"
							android:contentDescription="@string/image_view"
							tools:ignore="SpeakableTextPresentCheck,UnusedAttribute" />
						<WebView
							android:id="@+id/maindWebView"
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:visibility="gone"
							android:minWidth="50dp"
							android:minHeight="100dp"
							android:contentDescription="@string/video_view"
							tools:ignore="SpeakableTextPresentCheck" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/maindLinearLayoutTop"
						android:gravity="center"
						android:orientation="horizontal"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						tools:ignore="DisableBaselineAlignment">
						<LinearLayout
							android:background="@drawable/background_bottom"
							android:orientation="vertical"
							android:gravity="center"
							android:id="@+id/maindLinearLayout3"
							android:layout_width="0dp"
							android:layout_height="wrap_content"
							android:layout_weight="1.0"
							tools:ignore="UselessParent">
							<LinearLayout
								android:gravity="left"
								android:orientation="vertical"
								android:id="@+id/maindTextViewL1"
								android:background="@android:color/transparent"
								android:paddingLeft="5.0dp"
								android:paddingRight="5.0dp"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								tools:ignore="RtlHardcoded">
								<TextView
									android:textAppearance="?android:textAppearanceMedium"
									android:id="@+id/maindTextView1"
									android:gravity="right"
									android:background="@android:color/transparent"
									android:textColor="#ff000000"
									android:paddingLeft="2.0dip"
									android:paddingRight="2.0dip"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:textIsSelectable="false"
									tools:ignore="VisualLintBounds" />
							</LinearLayout>
							<LinearLayout
								android:gravity="right"
								android:orientation="vertical"
								android:id="@+id/maindTextViewL2"
								android:background="@android:color/transparent"
								android:paddingLeft="5.0dp"
								android:paddingBottom="2.0dip"
								android:paddingRight="5.0dp"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								tools:ignore="RtlHardcoded">
								<TextView
									android:textAppearance="?android:textAppearanceMedium"
									android:id="@+id/maindTextView2"
									android:gravity="left"
									android:background="@android:color/transparent"
									android:textColor="#ff000000"
									android:padding="2.0dp"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:textIsSelectable="false"/>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
	<LinearLayout
		android:id="@+id/maindLinearLayoutBottom"
		android:background="@android:color/transparent"
		android:backgroundTint="@android:color/transparent"
		android:gravity="center"
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="5dp"
		android:layout_marginRight="5dp"
		android:layout_marginTop="2dp"
		android:layout_marginBottom="5dp">

		<ImageButton
			android:id="@+id/maindButton3"
			android:layout_width="50.0dp"
			android:layout_height="50.0dp"
			android:layout_marginLeft="1.0dip"
			android:layout_marginRight="1.0dip"
			android:background="@drawable/button_f"
			android:src="@drawable/ic_video"
			android:contentDescription="@string/video"
			app:tint="@color/button_f"
			tools:ignore="ImageContrastCheck,RtlHardcoded,SpeakableTextPresentCheck" />

		<ImageButton
			android:id="@+id/maindButtonLeft"
			android:layout_width="0dp"
			android:layout_height="50.0dip"
			android:layout_weight="0.5"
			android:layout_marginLeft="1.0dip"
			android:layout_marginRight="1.0dip"
			android:background="@drawable/button_slide"
			android:src="@drawable/ic_arrow_left"
			android:contentDescription="@string/left"
			app:tint="@color/button_slide"
			tools:ignore="ImageContrastCheck,RtlHardcoded,SpeakableTextPresentCheck"
			android:importantForAccessibility="no" />

		<ImageButton
			android:id="@+id/maindButtonShare"
			android:layout_width="50.0dp"
			android:layout_height="50.0dp"
			android:layout_marginLeft="1.0dip"
			android:layout_marginRight="1.0dip"
			android:background="@drawable/button_share_inverse"
			android:src="@drawable/ic_share"
			android:contentDescription="@string/video"
			app:tint="@color/textPrimaryInverse"
			tools:ignore="ImageContrastCheck,RtlHardcoded,SpeakableTextPresentCheck" />

		<ImageButton
			android:id="@+id/maindButtonRight"
			android:layout_width="0dp"
			android:layout_height="50.0dp"
			android:layout_weight="0.5"
			android:layout_marginLeft="1.0dip"
			android:layout_marginRight="1.0dip"
			android:background="@drawable/button_slide"
			android:src="@drawable/ic_arrow_right"
			android:contentDescription="@string/right"
			app:tint="@color/button_slide"
			tools:ignore="ImageContrastCheck,RtlHardcoded,SpeakableTextPresentCheck" />

		<ImageButton
			android:id="@+id/maindButton2"
			android:layout_width="50.0dip"
			android:layout_height="50.0dp"
			android:layout_marginLeft="1.0dip"
			android:layout_marginRight="1.0dip"
			android:background="@drawable/button_d"
			android:src="@drawable/ic_image"
			android:contentDescription="@string/image"
			app:tint="@color/button_d"
			tools:ignore="RtlHardcoded,SpeakableTextPresentCheck" />
	</LinearLayout>
</LinearLayout>